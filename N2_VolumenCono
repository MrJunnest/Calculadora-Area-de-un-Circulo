/* Calcular el volumen de un cono. */
#include <stdio.h>
#include <conio.h>
#define Pi 3.14
int main ()
{
  float radio;
  float altura;
  float resultado;
  printf("Inserte el radio:\n");
  scanf("%f",&radio);
  printf("Inserte la altura\n");
  scanf("%f",&altura);
  resultado=1/((3*Pi)*(radio*radio)*altura);
  printf("El volumen del cono es:%f\n",resultado);
  getch ();
  return 0;
}
