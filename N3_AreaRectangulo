/* Para encontrar el Area de un Rectangulo */
#include <stdio.h>
#include <conio.h>
int main ()
{
  float altura;
  float base;
  float resultado;
  printf("Inserte la altura del Rectangulo\n");
  scanf("%f",&altura);
  printf("Inserte la base del Rectangulo\n");
  scanf("%f",&base);
  resultado=altura*base;
  printf("El Area del rectangulo es:%f\n",resultado);
  getch ();
  return 0;
}
