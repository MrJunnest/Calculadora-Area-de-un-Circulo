/* Calcular el interes Anual.*/
#include <stdio.h>
#include <conio.h>
int main ()
{
  float deposito;
  float resultado;
  float edad;
  printf("Inserte el deposito que ha hecho\n");
  scanf("%f",&deposito);
  printf("Cuanto tiempo ha sido?\n");
  scanf("%f",&edad);
  resultado=(deposito*(1+7)*edad);
  printf("El Interes Compuesto es:%f\n",resultado);
  getch ();
  return 0;
}
