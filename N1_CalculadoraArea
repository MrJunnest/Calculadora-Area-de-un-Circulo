/* Calcular el Area del Circulo. */
#include <stdio.h>
#include <conio.h>
#define Pi 3.14
int main ()
{
  float dato;
  float result;
  printf("Inserte el radio del circulo, para calcular el Area\n");
  scanf("%f",&dato);
  result=Pi*(dato*dato);
  printf("El Area del circulo es:%f\n",result);
  getch ();
  return 0;
}
